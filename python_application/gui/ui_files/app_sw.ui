  <property name="styleSheet">
   <string notr="true">/* -----------------------------------------
   GENERAL - Modern Glassmorphism UI
----------------------------------------- */
QWidget {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 rgba(150, 50, 100, 0.9),   /* Dark pink on the left */
                                stop:1 rgba(80, 40, 140, 0.9));  /* Purple on the right */
    font-family: -apple-system, BlinkMacSystemFont, &quot;Segoe UI&quot;, &quot;Roboto&quot;, &quot;Helvetica&quot;, sans-serif;
    font-size: 14px;
    color: #E8E0F5;                 /* Light purple text */
}

/* -----------------------------------------
   BUTTONS (GENERAL)
----------------------------------------- */
QPushButton {
    background-color: rgba(255, 255, 255, 0.12);  /* Glass-like translucent */
    color: #F5F0FF;
    border: 1px solid rgba(255, 255, 255, 0.25);
    border-radius: 6px;
    padding: 0px;
    font-weight: 600;
    font-size: 14px;
    min-height: 36px;
}

QPushButton:hover {
    background-color: rgba(255, 255, 255, 0.2);
    border: 1px solid rgba(255, 255, 255, 0.4);
    box-shadow: 0 8px 24px rgba(180, 120, 220, 0.6);
}

QPushButton:pressed {
    background-color: rgba(255, 255, 255, 0.15);
    transform: scale(0.98);
    box-shadow: 0 4px 12px rgba(200, 150, 220, 0.3);
}

/* Disabled state */
QPushButton:disabled {
    background-color: rgba(180, 170, 190, 0.3);
    color: rgba(45, 36, 64, 0.4);
    border: 1px solid rgba(255, 255, 255, 0.2);
}

QPushButton::icon {
    alignment: center;
}

/* Secondary button style */
QPushButton[secondary=&quot;true&quot;] {
    background-color: rgba(255, 255, 255, 0.08);
    color: #F5F0FF;
    border: 1px solid rgba(255, 255, 255, 0.2);
}

QPushButton[secondary=&quot;true&quot;]:hover {
    background-color: rgba(255, 255, 255, 0.15);
    border: 1px solid rgba(255, 255, 255, 0.35);
    box-shadow: 0 8px 24px rgba(255, 255, 255, 0.2);
}

/* -----------------------------------------
   SIDEBAR
----------------------------------------- */
QFrame#sidebar {
    background-color: rgba(255, 255, 255, 0.08);  /* Very translucent to see background through */
    border-right: 1px solid rgba(200, 160, 220, 0.3);
    border-radius: 0px;
    padding: 20px 1px;
}

/* Logo styling */
QLabel#logo_gif {
    background-color: transparent;
    border-radius: 12px;
    padding: 16px 0px;
}

/* -----------------------------------------
   SIDEBAR BUTTONS (Glowing Effect)
----------------------------------------- */
QPushButton#filter_pg_btn,
QPushButton#gallery_pg_btn,
QPushButton#profile_pg_btn {
    background-color: transparent;
    border: none;
    color: #2D2440;
    padding: 14px 16px;
    border-radius: 14px;
    text-align: left;
    font-weight: 500;
    font-size: 15px;
}

QPushButton#filter_pg_btn:hover,
QPushButton#gallery_pg_btn:hover,
QPushButton#profile_pg_btn:hover {
    background-color: rgba(220, 180, 240, 0.3);
    box-shadow: 0 0 20px rgba(200, 150, 230, 0.6),
                0 0 40px rgba(200, 150, 230, 0.4),
                inset 0 0 20px rgba(255, 255, 255, 0.2);
    border: 1px solid rgba(230, 190, 250, 0.5);
}

QPushButton#filter_pg_btn,
QPushButton#gallery_pg_btn,
QPushButton#profile_pg_btn {
    padding: 0px;                  /* Remove padding offsets */
    text-align: center;            /* Center icon */
    qproperty-iconSize: 28px 28px; /* (Optional) define icon size */
}

/* Active/selected state */
QPushButton#filter_pg_btn[active=&quot;true&quot;],
QPushButton#gallery_pg_btn[active=&quot;true&quot;],
QPushButton#profile_pg_btn[active=&quot;true&quot;] {
    font-weight: 700;
    background-color: rgba(220, 180, 240, 0.4);
    box-shadow: 0 0 15px rgba(200, 150, 230, 0.5);
}

/* -----------------------------------------
   COMBO BOX
----------------------------------------- */
QComboBox {
    background-color: rgba(255, 255, 255, 0.5);
    border: 1px solid rgba(255, 255, 255, 0.4);
    border-radius: 12px;
    padding: 10px 36px 10px 14px;
    color: #2D2440;
    font-weight: 500;
    font-size: 14px;
    min-height: 36px;
}

QComboBox:hover {
    border: 1px solid rgba(220, 180, 230, 0.6);
    background-color: rgba(255, 255, 255, 0.7);
    box-shadow: 0 4px 16px rgba(220, 180, 230, 0.3);
}

QComboBox:focus {
    border: 1px solid rgba(220, 180, 230, 0.8);
    box-shadow: 0 0 20px rgba(220, 180, 230, 0.4);
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 32px;
    border-left: none;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
}

QComboBox::down-arrow {
    image: url(:/icons/arrow_down.svg);
    width: 12px;
    height: 12px;
}

QComboBox QAbstractItemView {
    background-color: rgba(250, 245, 255, 0.95);
    border: 1px solid rgba(220, 180, 230, 0.5);
    border-radius: 12px;
    selection-background-color: rgba(220, 180, 240, 0.4);
    selection-color: #2D2440;
    outline: 0;
    padding: 6px;
}

QComboBox QAbstractItemView::item {
    min-height: 38px;
    padding: 10px 14px;
    color: #2D2440;
    border-radius: 8px;
}

QComboBox QAbstractItemView::item:hover {
    background-color: rgba(220, 180, 240, 0.3);
    box-shadow: 0 0 12px rgba(220, 180, 230, 0.4);
}

/* -----------------------------------------
   INPUT FIELDS (Glassmorphism)
----------------------------------------- */
QLineEdit, QTextEdit {
    background-color: rgba(255, 255, 255, 0.5);
    border: 1px solid rgba(255, 255, 255, 0.4);
    border-radius: 12px;
    padding: 11px 14px;
    color: #2D2440;
    font-size: 14px;
}

QLineEdit:focus, QTextEdit:focus {
    background-color: rgba(255, 255, 255, 0.7);
    border: 1px solid rgba(220, 180, 230, 0.8);
    box-shadow: 0 0 20px rgba(220, 180, 230, 0.4);
    outline: none;
}

QLineEdit::placeholder, QTextEdit::placeholder {
    color: rgba(45, 36, 64, 0.5);
}

/* -----------------------------------------
   SCROLLBARS (Glassmorphism)
----------------------------------------- */
QScrollBar:vertical {
    background: rgba(255, 255, 255, 0.2);
    width: 10px;
    border-radius: 5px;
    margin: 2px;
}

QScrollBar::handle:vertical {
    background: rgba(220, 180, 230, 0.5);
    border-radius: 5px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background: rgba(220, 180, 230, 0.8);
    box-shadow: 0 0 10px rgba(200, 150, 230, 0.6);
}

QScrollBar::add-line:vertical, 
QScrollBar::sub-line:vertical {
    height: 0px;
    background: none;
}

QScrollBar:horizontal {
    background: rgba(255, 255, 255, 0.2);
    height: 10px;
    border-radius: 5px;
    margin: 2px;
}

QScrollBar::handle:horizontal {
    background: rgba(220, 180, 230, 0.5);
    border-radius: 5px;
    min-width: 30px;
}

QScrollBar::handle:horizontal:hover {
    background: rgba(220, 180, 230, 0.8);
    box-shadow: 0 0 10px rgba(200, 150, 230, 0.6);
}

/* -----------------------------------------
   TABLE VIEWS
----------------------------------------- */
QTableView {
    background-color: rgba(255, 255, 255, 0.4);
    gridline-color: rgba(220, 180, 230, 0.2);
    color: #2D2440;
    border: 1px solid rgba(255, 255, 255, 0.4);
    border-radius: 12px;
    selection-background-color: rgba(220, 180, 240, 0.3);
    selection-color: #2D2440;
}

QHeaderView::section {
    background-color: rgba(240, 235, 250, 0.6);
    color: #2D2440;
    border: none;
    border-bottom: 1px solid rgba(220, 180, 230, 0.3);
    padding: 12px 10px;
    font-weight: 600;
    font-size: 12px;
    text-transform: uppercase;
    letter-spacing: 0.8px;
}

/* -----------------------------------------
   CARDS / FRAMES
----------------------------------------- */
QFrame[card=&quot;true&quot;] {
    background-color: rgba(255, 255, 255, 0.5);
    border: 1px solid rgba(255, 255, 255, 0.4);
    border-radius: 16px;
    box-shadow: 0 8px 32px rgba(200, 150, 230, 0.2);
}

/* -----------------------------------------
   SIDEBAR (Rounded Glass Panel)
----------------------------------------- */
QFrame#sidebar {
    background-color: rgba(255, 255, 255, 0.08);
    border-right: 1px solid rgba(200, 160, 220, 0.3);

    border-radius: 10px;     /* Round the corners */
    margin: 6px;            /* Push away from window so radius is visible */
    padding: 20px 1px;
}


/* -----------------------------------------
   LABELS (Headers)
----------------------------------------- */
QLabel[heading=&quot;true&quot;] {
    font-weight: 700;
    font-size: 18px;
    color: #2D2440;
}

QLabel[subheading=&quot;true&quot;] {
    font-weight: 500;
    font-size: 14px;
    color: rgba(45, 36, 64, 0.7);
}

/* -----------------------------------------
   SEPARATORS
----------------------------------------- */
QFrame[separator=&quot;true&quot;] {
    background-color: rgba(220, 180, 230, 0.3);
    max-height: 1px;
    min-height: 1px;
}

/* -----------------------------------------
   TABS (Glassmorphism)
----------------------------------------- */
QTabWidget::pane {
    border: none;
    background-color: rgba(255, 255, 255, 0.4);
    border-radius: 12px;
}

QTabBar::tab {
    background-color: transparent;
    color: rgba(45, 36, 64, 0.6);
    border: none;
    border-bottom: 2px solid transparent;
    padding: 12px 20px;
    font-weight: 600;
    font-size: 14px;
}

QTabBar::tab:hover {
    color: #2D2440;
    background-color: rgba(220, 180, 240, 0.2);
    box-shadow: 0 0 15px rgba(200, 150, 230, 0.4);
}

QTabBar::tab:selected {
    color: #2D2440;
    border-bottom: 2px solid rgba(200, 150, 230, 0.8);
    background-color: rgba(220, 180, 240, 0.3);
}

/* -----------------------------------------
   CHECKBOXES &amp; RADIO BUTTONS
----------------------------------------- */
QCheckBox, QRadioButton {
    color: #2D2440;
    spacing: 10px;
}

QCheckBox::indicator, QRadioButton::indicator {
    width: 20px;
    height: 20px;
    border-radius: 5px;
    border: 2px solid rgba(220, 180, 230, 0.5);
    background-color: rgba(255, 255, 255, 0.5);
}

QCheckBox::indicator:hover, QRadioButton::indicator:hover {
    border: 2px solid rgba(220, 180, 230, 0.8);
    box-shadow: 0 0 12px rgba(200, 150, 230, 0.5);
}

QCheckBox::indicator:checked {
    background-color: rgba(220, 180, 230, 0.8);
    border: 2px solid rgba(200, 150, 230, 0.9);
    image: url(:/icons/check_white.svg);
    box-shadow: 0 0 15px rgba(200, 150, 230, 0.6);
}

QRadioButton::indicator {
    border-radius: 10px;
}

QRadioButton::indicator:checked {
    background-color: rgba(220, 180, 230, 0.8);
    border: 5px solid rgba(255, 255, 255, 0.9);
    box-shadow: 0 0 15px rgba(200, 150, 230, 0.6);
}

/* -----------------------------------------
   TOOLTIPS
----------------------------------------- */
QToolTip {
    background-color: rgba(240, 235, 250, 0.95);
    color: #2D2440;
    border: 1px solid rgba(220, 180, 230, 0.5);
    border-radius: 8px;
    padding: 8px 12px;
    font-size: 12px;
    box-shadow: 0 4px 16px rgba(200, 150, 230, 0.3);
}

/* -----------------------------------------
   MENU &amp; CONTEXT MENUS
----------------------------------------- */
QMenu {
    background-color: rgba(250, 245, 255, 0.95);
    border: 1px solid rgba(220, 180, 230, 0.5);
    border-radius: 12px;
    padding: 6px;
}

QMenu::item {
    padding: 10px 16px;
    border-radius: 8px;
    color: #2D2440;
}

QMenu::item:selected {
    background-color: rgba(220, 180, 240, 0.4);
    box-shadow: 0 0 12px rgba(200, 150, 230, 0.4);
}

QMenu::separator {
    height: 1px;
    background-color: rgba(220, 180, 230, 0.3);
    margin: 6px 10px;
}

/* -----------------------------------------
   DIALOGS &amp; MESSAGE BOXES
----------------------------------------- */
QDialog {
    background-color: rgba(240, 230, 250, 0.95);
}

QMessageBox {
    background-color: rgba(240, 230, 250, 0.95);
}

/* -----------------------------------------
   PROGRESS BARS
----------------------------------------- */
QProgressBar {
    background-color: rgba(255, 255, 255, 0.4);
    border: 1px solid rgba(220, 180, 230, 0.3);
    border-radius: 6px;
    height: 8px;
    text-align: center;
}

QProgressBar::chunk {
    background-color: rgba(220, 180, 230, 0.8);
    border-radius: 5px;
    box-shadow: 0 0 10px rgba(200, 150, 230, 0.5);
}

/*side bar stuff here change it if needed*/
/* -----------------------------------------
   SCROLL AREA - Sidebar-style Glass
----------------------------------------- */
QScrollArea{
    background-color: rgba(255, 255, 255, 0.08);  /* Very translucent like sidebar */
    border: 1px solid rgba(200, 160, 220, 0.3);   /* Subtle glass border */
    border-radius: 10px;                           /* Rounded corners to match sidebar */
    padding: 10px;                                 /* Inner padding */
}

QScrollArea{
    background-color: transparent;                /* Keep the inner container transparent */
    border-radius: 10px;                           /* Match outer radius */
}

/* Optional: Scrollbars in sidebar style */
QScrollBar:vertical {
    background: rgba(255, 255, 255, 0.2);
    width: 10px;
    border-radius: 5px;
}

QScrollBar::handle:vertical {
    background: rgba(220, 180, 230, 0.5);
    border-radius: 5px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background: rgba(220, 180, 230, 0.8);
    box-shadow: 0 0 10px rgba(200, 150, 230, 0.6);
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;
    background: none;
}</string>
